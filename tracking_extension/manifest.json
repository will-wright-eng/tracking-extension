{
  "name": "Tracking Extension",
  "action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "version": "0.4.3",
  "description": "Tracking Extension",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
 "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png"
  }
}